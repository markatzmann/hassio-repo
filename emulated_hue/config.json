{
  "name": "Emulated HUE",
  "version": "0.3.0_Mark",
  "description": "Hue Emulation for Home Assistant",
  "url": "https://github.com/markatzmann/core",
  "slug": "emulated_hue",
  "stage": "stable",
  "startup": "application",
  "boot": "auto",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "map": ["config:rw"],
  "host_network": true,
  "environment": {},
  "homeassistant_api": true,
  "init": false,
  "options": {
    "verbose": false
  },
  "schema": {
    "http_port": "int?",
    "https_port": "int?",
    "use_default_ports_for_discovery": "bool?",
    "verbose": "bool"
  }
}
